<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Button name="ZQA_AbandonButton" virtual="true" registerForClicks="AnyUp" frameStrata="HIGH" frameLevel="6">
		<Size>
			<AbsDimension x="30" y="30"/>
		</Size>
		<NormalFont style="GameFontNormal"/>
		<NormalTexture file="Interface\Buttons\CancelButton-Up"/>
		<PushedTexture file="Interface\Buttons\CancelButton-Down"/>
		<HighlightTexture file="Interface\Buttons\CancelButton-Highlight" alphaMode="ADD"/>
		
		<Scripts>
			<OnEnter>
				ZQA_ButtonEnter(self)
			</OnEnter>
			<OnLeave>
				ZQA_ButtonLeave(self)
			</OnLeave>
			<OnClick>
				ZQA_ButtonClick(self)
			</OnClick>
		</Scripts>
	</Button>
</Ui>